{
  "name": "mattdell-mobiento",
  "version": "1.0.0",
  "description": "Matt Dell's Mobiento Demo",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack -p",
    "deploy:staging": "npm run build && surge --project ./dist --domain mattdell-mobiento-staging.surge.sh",
    "deploy:production": "npm run build && surge --project ./dist --domain mattdell-mobiento.surge.sh",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "NODE_ENV=development webpack-dev-server --hot --inline --port 3000 --colors --no-info",
    "test": "echo \"Warning: no tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsaiDavid/simple-redux-boilerplate.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/tsaiDavid/simple-redux-boilerplate",
  "devDependencies": {
    "@types/react": "^15.0.27",
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.16.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.0.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.4.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.0",
    "surge": "^0.19.0",
    "ts-loader": "^2.1.0"
  },
  "dependencies": {
    "history": "^2.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^4.1.1",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
