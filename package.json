{
  "name": "mattdell-mobiento",
  "version": "1.0.0",
  "description": "Matt Dell's Mobiento Demo",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack -p",
    "deploy:staging": "npm run build && surge --project ./dist --domain mattdell-mobiento-staging.surge.sh",
    "deploy:production": "npm run build && surge --project ./dist --domain mattdell-mobiento.surge.sh",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "NODE_ENV=development webpack-dev-server --hot --inline --port 3000 --colors",
    "test": "echo \"Warning: no tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsaiDavid/simple-redux-boilerplate.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/tsaiDavid/simple-redux-boilerplate",
  "devDependencies": {
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.27",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.46",
    "@types/react-router": "^3.0.5",
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.0.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.0",
    "surge": "^0.19.0",
    "ts-loader": "^2.1.0",
    "typescript-eslint-parser": "^4.0.0"
  },
  "dependencies": {
    "history": "^2.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^4.1.1",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.4.0",
    "typescript": "2.3.2",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
